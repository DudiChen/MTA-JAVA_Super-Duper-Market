<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import javafx.scene.text.Font?>
<fx:root type="javafx.scene.control.ListCell" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <graphic>
        <AnchorPane>
            <Pane AnchorPane.leftAnchor="0.0">
                <children>
                    <VBox >
                        <children>
                            <Label fx:id="nameLabel" alignment="TOP_LEFT">
                                <font>
                                    <Font name="System Bold Italic" size="17.0" />
                                </font>
                            </Label>
                            <Label fx:id="idLabel" text="ID: ">
                                <font>
                                    <Font size="10.0" />
                                </font>
                            </Label>
                            <Label fx:id="locationLabel" text="Location: ">
                                <font>
                                    <Font size="10.0" />
                                </font>
                            </Label>
                            <Label fx:id="totalOrdersLabel" text="Total orders: ">
                                <font>
                                    <Font size="10.0" />
                                </font>
                            </Label>
                            <Label fx:id="ordersAveragePriceLabel" text="Orders Average Price: ">
                                <font>
                                    <Font size="10.0" />
                                </font>
                            </Label>
                            <Label fx:id="averageShipmentPriceLabel" text="Average Shipment Price: ">
                                <font>
                                    <Font size="10.0" />
                                </font>
                            </Label>
                        </children>
                    </VBox>
                </children>
            </Pane>
        </AnchorPane>
    </graphic>
</fx:root>
